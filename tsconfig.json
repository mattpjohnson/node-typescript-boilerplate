  "compilerOptions": {
    "declaration": true,
    "outDir": "dist",
    "module": "commonjs",
    "esModuleInterop": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "types": ["jest", "node"],
    "target": "es5",
    "lib": ["dom", "es2019"]
  }
}
